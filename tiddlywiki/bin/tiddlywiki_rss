#!/bin/bash

set -e

tiddlywiki --load ~/phajas-wiki/phajas-wiki.html --render "[[$:/plugins/sq/feeds/templates/rss]]" "feed.xml" "text/plain" "$:/core/templates/wikified-tiddler"
mv output/feed.xml /tmp/
rm -r output

rsync -avvz /tmp/feed.xml peterhajas.com:/var/www/html/demo.xml
rm /tmp/feed.xml
